#!/bin/bash

export PYTHONPATH=.

algos=(
    happo_oc
    happo_oc
)
suite=$1
shift
# envs=(escalation staghunt harvest)
envs=(
    # HalfCheetah_2x3
    # Ant_2x4
    # Walker2d_2x3
    HalfCheetah_6x1
    Walker2d_6x1
)
# e=${suite}
configs=(
    "${suite}"
)
na=6
args1=(1 3)
args2=(
    # .9,.09,.009,.001 
    # .001,.9,.09,.009 
    # .009,.001,.9,.09 
