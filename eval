#!/bin/zsh

source activate grl

nohup python run/eval.py logs/gd/gd/batch_size=256-norm=None-use_ln=False-norm_after_activation=False -n 10000
nohup python run/eval.py logs/gd/gd/batch_size=256-norm=None-use_ln=True-norm_after_activation=False -n 10000 -nw 50 -ne 20
nohup python run/eval.py logs/gd/gd/batch_size=256-norm=layer-use_ln=False-norm_after_activation=False
nohup python run/eval.py logs/gd/gd/batch_size=256-norm=layer-use_ln=False-norm_after_activation=True
nohup python run/eval.py logs/gd/gd/batch_size=256-norm=layer-use_ln=True-norm_after_activation=False
nohup python run/eval.py logs/gd/gd/batch_size=256-norm=layer-use_ln=True-norm_after_activation=True
