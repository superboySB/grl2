#!/bin/zsh


algos=(
    # ppo
    # ppo2
    # ppg
    # apex-dqn
    apex-iqn
    # apex-mrdqn
    # apex-sacdiqn
    # apex-mrdqn
    # sacdiqn5
)
suite=procgen
envs=(
    # plunder
    # chaser
    # bigfish
    # starpilot
    coinrun
    # miner
    # caveflyer 
    # climber
    # dodgeball 
    # leaper
    # jumper
    # fruitbot
    # heist
    # ninja
    # bossfight
    # maze
)
args1=(0.01 0.005 0.001)
args2=(clip)
args3=(conv_maxblurpool)

for e in "${envs[@]}"; do
    for a in "${algos[@]}"; do
        for a1 in "${args1[@]}"; do
            for a2 in "${args2[@]}"; do
                for a3 in "${args3[@]}"; do
                    # if [[ $e = plunder && $a1 = None ]]
                    # then continue
                    # fi
                    # sed -e "s/KI/${a2}/g" \
                    #     algo/ppo/${suite}_template.yaml > algo/ppo/${suite}_config.yaml
                    dir="logs/${suite}_${e}/${a}"
                    mkdir -p $dir
                    # py_script="train ${a} -e ${suite}_${e} $@ >& ${dir}/outputs.txt"
                    py_script="train ${a} -e ${suite}_${e} -kw tau=$a1 >& ${dir}/outputs.txt"
                    echo $py_script
                    eval $py_script
                    last_dir=$(ls -td ${dir}/* | grep -v txt | head -n 1)
                    mv_script="mv ${dir}/outputs.txt ${last_dir}/outputs.txt"
                    echo $mv_script
                    eval $mv_script
                done
            done
        done
    done
done

